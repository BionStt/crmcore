<!DOCTYPE html>

<html>
	<head>
        <meta charset="utf-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        <title>CRM Core - @ViewData["Title"]</title>

		<link rel="stylesheet" href="http://coreui.io/demo/Static_Demo/vendors/css/font-awesome.min.css" />
		<link rel="stylesheet" href="http://coreui.io/demo/Static_Demo/vendors/css/simple-line-icons.min.css" />
        <link rel="stylesheet" href="~/styles/style.css" />
    </head>
	<body class="app flex-row align-items-center">
        <div class="container body-content">
            @RenderBody()
        </div>

		 <script src="https://ajax.aspnetcdn.com/ajax/jquery/jquery-2.1.4.min.js"></script>
        <script src="https://ajax.aspnetcdn.com/ajax/jquery.validate/1.16.0/jquery.validate.min.js"
            asp-fallback-src="~/lib/jquery-validation/dist/jquery.validate.min.js"
            asp-fallback-test="window.jQuery && window.jQuery.validator">
    </script>
    <script src="https://ajax.aspnetcdn.com/ajax/jquery.validation.unobtrusive/3.2.6/jquery.validate.unobtrusive.min.js"
            asp-fallback-src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"
            asp-fallback-test="window.jQuery && window.jQuery.validator && window.jQuery.validator.unobtrusive">
    </script>
    @RenderSection("scripts", required: false)
	</body>
</html>